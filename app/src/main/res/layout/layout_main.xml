<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <ImageView        android:id="@+id/imgExport"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="80dp"        android:layout_marginTop="60dp"        android:layout_marginEnd="60dp"        android:clickable="true"        android:src="@drawable/ic_export_64"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/textView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="80dp"        android:layout_marginTop="30dp"        android:text="Xuất hàng"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imgExport" />    <ImageView        android:id="@+id/imgImport"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="60dp"        android:layout_marginEnd="80dp"        android:clickable="true"        android:src="@drawable/ic_import_64"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/textView2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="30dp"        android:layout_marginEnd="76dp"        android:text="Nhập hàng"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imgImport" />    <ImageView        android:id="@+id/imgStatistics"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="80dp"        android:layout_marginTop="28dp"        android:clickable="true"        android:src="@drawable/ic_statistics_64"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView" />    <TextView        android:id="@+id/textView3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="80dp"        android:layout_marginTop="30dp"        android:text="Thống kê"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imgStatistics" />    <ImageView        android:id="@+id/imgCustomer"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="30dp"        android:layout_marginEnd="80dp"        android:clickable="true"        android:src="@drawable/ic_customer_64"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView2" />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="30dp"        android:layout_marginEnd="68dp"        android:text="Khách hàng"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imgCustomer" />    <ImageView        android:id="@+id/imgProduct"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="80dp"        android:layout_marginTop="30dp"        android:clickable="true"        android:src="@drawable/ic_product_64"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView3" />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="80dp"        android:layout_marginTop="30dp"        android:text="Sản phẩm"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imgProduct" /></androidx.constraintlayout.widget.ConstraintLayout>